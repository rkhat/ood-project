<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
  prefHeight="200.0" prefWidth="300.0" spacing="30.0"
  stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="controllers.ChangePasswordController">
  <children>
    <JFXPasswordField fx:id="currentPasswordField"
      promptText="Current Password" />
    <JFXPasswordField fx:id="newPasswordField"
      promptText="New Password" />
    <JFXPasswordField fx:id="newPasswordField2"
      promptText="Confirm New Password" />
    <JFXButton fx:id="changePasswordButton"
      onAction="#changePasswordAction" text="Change Password" />
  </children>
  <padding>
    <Insets bottom="30.0" left="20.0" right="20.0" top="30.0" />
  </padding>
</VBox>
