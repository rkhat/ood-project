<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
 fx:controller="controllers.SignupController" styleClass="form">
 <children>
  <JFXTextField fx:id="usernameField" styleClass="form-field"
   onAction="#signupAction" promptText="Username" />
  <JFXPasswordField fx:id="passwordField"
   styleClass="form-field" onAction="#signupAction" promptText="Password" />
  <JFXPasswordField fx:id="passwordField2"
   styleClass="form-field" onAction="#signupAction"
   promptText="Confirm Password" />
  <JFXButton fx:id="signupButton" styleClass="form-button"
   onAction="#signupAction" text="Signup" />
 </children>
</VBox>
