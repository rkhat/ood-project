<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
 fx:controller="controllers.InitialController" styleClass="initial">
 <children>
  <Label styleClass="initial-title" text="Welcome" />
  <Region styleClass="initial-spacing" />
  <HBox styleClass="initial-hbox">
   <children>
    <JFXButton onAction="#memberAction" styleClass="initial-button"
     text="Member" />
    <JFXButton onAction="#registerAction" styleClass="initial-button"
     text="Register" />
   </children>
  </HBox>
 </children>
</VBox>
